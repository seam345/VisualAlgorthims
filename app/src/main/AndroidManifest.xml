<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uk.co.cyborgapps.visualalgorithms" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="uk.co.cyborgapps.visualalgorithms.AlgorthimListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="uk.co.cyborgapps.visualalgorithms.AlgorthimDetailActivity"
            android:label="@string/title_algorthim_detail"
            android:parentActivityName="uk.co.cyborgapps.visualalgorithms.AlgorthimListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="uk.co.cyborgapps.visualalgorithms.AlgorthimListActivity" />
        </activity>
    </application>

</manifest>
